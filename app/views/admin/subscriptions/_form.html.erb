<%= f.select :emote_amount, [1, 5, 10, 25] %><br/>
<%= f.label :start_date, 'Start date' %>
<%= f.text_field :start_date %><br/>
<%= f.label :end_date, 'End date' %>
<%= f.text_field :end_date %><br/>
<%= errors_for(subscription) %>