<%= f.label :user_id, 'For user' %>
<%= f.select :user_id, users.map {|u| [u.email, u.id]}, (params[:user_id].nil? ? {}  : {:selected => params[:user_id]} ) %>
<br/>
<%= f.label :project_name, 'Subject' %>
<%= f.text_field :project_name %>
<br/>
<%= f.label :code %>
<%= f.text_field :code %> (leave blank for random)
<br/><br/>
<%= f.radio_button :state, Survey::STATE_ACTIVE %> Active<br/>
<%= f.radio_button :state, Survey::STATE_ARCHIVED %> Archived<br/>
<%= f.radio_button :state, Survey::STATE_SUSPENDED %> Suspended<br/>
<%= errors_for(emote) %>