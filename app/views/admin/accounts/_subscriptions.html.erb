<table width="60%">
        <% columns = ['Date Purchased', 'Description', h('# of emotes'), h('Transaction #'), 'Paid', 'Starts', 'Expires', 'Action'] %>
	<tr>
          <% columns.each do |header| %>
            <th align="left"><%= header %></th>
          <% end %>
	</tr>
	<% subscriptions.each do |subscription| %>
          <tr>
            <td><%= subscription.purchase_date %></td>
            <td><%= raw subscription.description %></td>
            <td><%= subscription.emote_amount %></td>
            <td><%= subscription.token %></td>
            <td><%= subscription.total_paid %></td>
            <td><%= subscription.start_date.to_s(:short) %></td>
            <td><%= subscription.end_date.to_s(:short) %></td>
            <td><%= link_to 'Edit', edit_admin_subscription_path(subscription) %></td>
          </tr>
	<% end %>
        <tr>
          <% (columns.size-1).times { %> <td>&nbsp;</td> <% } %>
          <td>
            <%= link_to 'New', new_admin_subscription_path(:user_id => user_id) %>
          </td>
        </tr>
</table>
